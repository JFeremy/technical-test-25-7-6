else
  export NODE_PATH="/Users/jeremyfumeron/Documents/sideWorkspace/NestJs/gladia-tt/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_7b2ade1e23be19838bcb6c55b03f96a2/node_modules/ts-jest/node_modules:/Users/jeremyfumeron/Documents/sideWorkspace/NestJs/gladia-tt/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_7b2ade1e23be19838bcb6c55b03f96a2/node_modules:/Users/jeremyfumeron/Documents/sideWorkspace/NestJs/gladia-tt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
